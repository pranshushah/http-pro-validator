{
  "name": "@http-pro/validator",
  "version": "0.1.0",
  "description": "light weight Yup and Zod validator function for http-pro",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/yup": "^0.32.0",
    "size-limit": "^7.0.8",
    "tsup": "^7.2.0",
    "typescript": "^4.3.4",
    "yup": "^1.3.2",
    "zod": "^3.22.4"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "default": "./dist/index.d.ts"
      },
      "default": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      },
      "browser": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "tsup",
    "analyze": "size-limit"
  },
  "keywords": [
    "fetch",
    "http-pro",
    "https",
    "axios",
    "scehma-validator",
    "yup",
    "zod",
    "ky",
    "URL",
    "network",
    "rest",
    "ajax",
    "xmlhttprequest",
    "api",
    "browser"
  ],
  "bugs": {
    "email": "pranshu.shah23@gmail.com",
    "url": "https://github.com/pranshushah/http-pro-validator/issues/new"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pranshushah/http-pro-validator"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "",
  "license": "ISC",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "2 KB"
    }
  ]
}
